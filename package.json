    ]
  },
  "devDependencies": {
    "@types/cli-progress": "^3.9.2",
    "@types/node": "^16.10.2",
    "@types/readline-sync": "^1.4.4",
    "@types/terminal-kit": "^1.28.4",
    "@types/webtorrent": "^0.109.2",
    "@types/yargs": "^17.0.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "cli-progress": "^3.9.1",
    "colors": "^1.4.0",
    "fs": "^0.0.1-security",
    "os": "^0.1.2",
    "parse-torrent": "^9.1.4",
    "path": "^0.12.7",
    "readline-sync": "^1.4.10",
    "terminal-kit": "^2.1.6",
    "tty-table": "^4.1.5",
    "webtorrent": "^1.5.6",
    "yargs": "^17.2.1"
  }
