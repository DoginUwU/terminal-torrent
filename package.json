  "version": "0.01",
  "main": "build/index.js",
  "bin": "build/index.js",
  "license": "MIT",
  "scripts": {
    "compile": "tsc --build && node build/index.js",
    "build": "tsc --build && pkg --output dist/terminal_torrent ."
